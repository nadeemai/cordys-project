<mvc:View
  xmlns:mvc="sap.ui.core.mvc"
  xmlns="sap.m"
  xmlns:core="sap.ui.core"
  xmlns:layout="sap.ui.layout"
  xmlns:form="sap.ui.layout.form"
  xmlns:uxap="sap.uxap"
  xmlns:u="sap.ui.unified"
  controllerName="com.cordys.cordys.controller.cordys">

  <Page id="SupplierRegistration" class="custom-scope" title="SPECIAL SANCTION APPROVAL PROCESS-Finance" showNavButton="true" navButtonPress="onBackDashboardpage">
    <uxap:ObjectPageLayout id="ObjectPageLayout" showTitleInHeaderContent="true" upperCaseAnchorBar="false" showHeaderContent="true" showFooter="true">
      <uxap:headerTitle>
        <uxap:ObjectPageDynamicHeaderTitle id="ObjectPageDynamicHeaderTitle">
          <uxap:actions>
            <Button id="btnSaveHeader" text="Save" icon="sap-icon://save" type="Accept" press="onSavePress"/>
            <Button id="btnSubmitHeader" text="Submit" icon="sap-icon://accept" type="Accept" press="onSubmitPress"/>
          </uxap:actions>
        </uxap:ObjectPageDynamicHeaderTitle>
      </uxap:headerTitle>

      <uxap:headerContent>
        <VBox id="_IDGenHBox1" justifyContent="Start">
          <form:SimpleForm
            id="SSimpleFormToolbar3"
            editable="true"
            layout="ResponsiveGridLayout"
            labelSpanXL="12"
            labelSpanL="12"
            labelSpanM="12"
            columnsXL="4"
            columnsL="4"
            columnsM="1"
            adjustLabelSpan="false"
            class="sapUiSmallMarginBeginEnd">
            <form:content>
              <Label id="_IDGenLabel" text="{i18n>Reference Number}" class="noBoldLabel"/>
              <Input id="inputRefNumber" value="FY-2024-3850" width="100%" editable="false"/>
              <Label id="_IDGenLabel1" text="{i18n>Location}"/>
              <ComboBox id="comboLocation" width="100%" selectedKey="ENV">
                <items>
                  <core:Item id="_IDGenItem" text="ENV" key="ENV"/>
                  <core:Item id="_IDGenItem1" text="Other" key="Other"/>
                </items>
              </ComboBox>
              <Label id="_IDGenLabel2" text="{i18n>Budget Requirment}" class="noBoldLabel"/>
              <Input id="inputBudget" value="2000" width="100%" type="Number" editable="false"/>
              <Label id="_IDGenLabel3" text="{i18n>Implement Date}"/>
              <DatePicker id="dateImplement" value="2024-10-18" displayFormat="dd/MM/yyyy" valueFormat="yyyy-MM-dd" width="100%"/>
              <core:Title id="_IDGenTitle2" class="noBoldLabel"/>
              <Label id="_IDGenLabel35" text="{i18n>Division}" class="noBoldLabel"/>
              <ComboBox id="Division" width="100%" selectedKey="FD" editable="false">
                <items>
                  <core:Item id="_IDGenItem2" text="FD" key="FD"/>
                  <core:Item id="_IDGenItem3" text="Other" key="Other"/>
                </items>
              </ComboBox>
              <Label id="_IDGenLabel5" text="{i18n>Project Name}" class="noBoldLabel"/>
              <Input id="inputProjectName" value="TEST 123" width="100%"/>
              <Label id="_IDGenLabel6" text="{i18n>IRR}"/>
              <Input id="inputIRR" value="1234" width="100%" type="Number"/>
              <core:Title id="_IDGenTitle3" class="noBoldLabel"/>
              <Label id="_IDGenLabelDepartment" text="{i18n>Department}" class="noBoldLabel"/>
              <ComboBox id="Department" width="100%" selectedKey="FD CDMM">
                <items>
                  <core:Item id="_IDGenItem4" text="FD CDMM" key="FD CDMM"/>
                  <core:Item id="_IDGenItem5" text="Other" key="Other"/>
                </items>
              </ComboBox>
              <Label id="_IDGenLabel8" text="{i18n>Item Required}" class="noBoldLabel"/>
              <Input id="inputItemRequired" value="TEST DESCRIPTION" width="100%"/>
              <Label id="_IDGenLabel9" text="{i18n>Market}"/>
              <ComboBox id="comboMarket" width="100%" selectedKey="Domestic (Dom)">
                <items>
                  <core:Item id="_IDGenItem6" text="Domestic (Dom)" key="Domestic (Dom)"/>
                  <core:Item id="_IDGenItem7" text="Other" key="Other"/>
                </items>
              </ComboBox>
              <core:Title id="_IDGenTitle4" class="noBoldLabel"/>
              <Label id="_IDGenLabelHod" text="{i18n>Hod}" class="noBoldLabel"/>
              <ComboBox id="Hod" width="100%" selectedKey="50914">
                <items>
                  <core:Item id="_IDGenItem8" text="50914 - E DHARANIKA - MIBS" key="50914"/>
                  <core:Item id="_IDGenItem9" text="Other" key="Other"/>
                </items>
              </ComboBox>
              <Label id="_IDGenLabel11" text="{i18n>Date}"/>
              <DatePicker id="dateField" value="2025-05-20" displayFormat="dd-MM-yyyy" valueFormat="yyyy-MM-dd" width="100%" editable="false"/>
              <Label id="_IDGenLabel12" text="{i18n>Hour}"/>
              <Input id="inputHour" value="12345" width="100%" type="Number"/>
            </form:content>
          </form:SimpleForm>
        </VBox>
      </uxap:headerContent>
      <uxap:sections>
        <uxap:ObjectPageSection id="_IDGenObjectPageSection" title="Justification Details">
          <uxap:subSections>
            <uxap:ObjectPageSubSection id="_IDGenObjectPageSubSection">
              <VBox id="_IDGenVBox" class="sapUiSmallMargin">
                <HBox id="_IDGenHBox3" width="100%" justifyContent="SpaceBetween" wrap="Wrap">
                  <VBox id="_IDGenVBox1" width="30%" class="sapUiSmallMargin">
                    <Label id="_IDGenLabel13" text="{i18n>BackgroundCurrentScenario}" class="redLabel"/>
                    <TextArea id="_IDGenTextArea" value="FOR TESTING" width="100%" rows="2" maxLength="1500"/>
                  </VBox>
                  <VBox id="_IDGenVBox2" width="30%" class="sapUiSmallMargin">
                    <Label id="_IDGenLabel14" text="{i18n>ProposalJustification}" class="redLabel"/>
                    <TextArea id="_IDGenTextArea1" value="FOR TESTING 123" width="100%" rows="2" maxLength="1500"/>
                  </VBox>
                  <VBox id="_IDGenVBox3" width="30%" class="sapUiSmallMargin">
                    <Label id="_IDGenLabel15" text="{i18n>ExpectedOutcome}" class="redLabel"/>
                    <TextArea id="_IDGenTextArea2" value="EXPECTED OUTCOME TEXT" width="100%" rows="2" maxLength="1500"/>
                  </VBox>
                </HBox>
              </VBox>
            </uxap:ObjectPageSubSection>
          </uxap:subSections>
        </uxap:ObjectPageSection>
        <uxap:ObjectPageSection id="_IDGenObjectPageSection1" title="Budget Details">
          <uxap:subSections>
            <uxap:ObjectPageSubSection id="_IDGenObjectPageSubSection1">
              <VBox id="_IDGenVBox4" class="sapUiSmallMargin">
                <Table id="_IDGenTable1" items="{budgetModel>/items}">
                  <columns>
                    <Column id="_IDGenColumn"><Text id="_IDGenText3" text="NATURE OF BUDGET"/></Column>
                    <Column id="_IDGenColumn1"><Text id="_IDGenText4" text="BUDGET AMOUNT"/></Column>
                    <Column id="_IDGenColumn2"><Text id="_IDGenText5" text="5% CONTINGENCY"/></Column>
                    <Column id="_IDGenColumn3"><Text id="_IDGenText6" text="TOTAL"/></Column>
                  </columns>
                  <items>
                    <ColumnListItem id="_IDGenColumnListItem">
                      <cells>
                        <Text id="_IDGenText7" text="{budgetModel>nature}"/>
                        <Input id="_IDGenInput" 
                               value="{budgetModel>amount}" 
                               type="Number" 
                               change="onBudgetAmountChange" 
                               editable="{= ${budgetModel>nature} !== 'Total' }"/>
                        <Text id="_IDGenText8" text="{budgetModel>contingency}"/>
                        <Text id="_IDGenText9" text="{budgetModel>total}"/>
                      </cells>
                    </ColumnListItem>
                  </items>
                </Table>
                <Text id="_IDGenText10" text="All values in ₹ Lacs." class="sapUiTinyMarginTop"/>
              </VBox>
            </uxap:ObjectPageSubSection>
          </uxap:subSections>
        </uxap:ObjectPageSection>

        <uxap:ObjectPageSection id="_IDGenObjectPageSection2" title="Attachment">
          <uxap:subSections>
            <uxap:ObjectPageSubSection id="_IDGenObjectPageSubSection2">
              <VBox id="_IDGenVBox5" class="sapUiSmallMargin" width="100%">
                <Table id="_IDGenTable2" items="{UploadDocSrvTabData>/attachments}">
                  <headerToolbar>
                    <OverflowToolbar id="_IDGenOverflowToolbar1">
                      <Text id="_IDGenTextAttachmentCount" 
                            text="Attachments: {= ${UploadDocSrvTabData>/attachments}.length }" 
                            class="sapUiTinyMarginEnd"/>
                            <ToolbarSpacer id="_IDGenToolbarSpacer1"/>
                      <u:FileUploader id="fileUploaderTabAttchment"
                        fileType="jpg,png,pdf,docx,jpeg,csv,xlsx,eml"
                        multiple="true"
                        icon="sap-icon://upload"
                        buttonText="Upload"
                        buttonOnly="true"
                        change="onUploadTabAttchmment"
                        class="sapUiSmallMarginEnd">
                        <u:customData>
                          <core:CustomData key="parentType" value="Request" writeToDom="false"/>
                        </u:customData>
                      </u:FileUploader>
                    </OverflowToolbar>
                  </headerToolbar>
                  <columns>
                    <Column id="_IDGenColumn4"><Text id="_IDGenText11" text="ID"/></Column>
                    <Column id="_IDGenColumn5"><Text id="_IDGenText12" text="File Name"/></Column>
                    <Column id="_IDGenColumn8" hAlign="End" width="6rem"><Text id="_IDGenText16" text="Action"/></Column>
                  </columns>
                  <items>
                    <ColumnListItem id="_IDGenColumnListItem1">
                      <cells>
                        <Text id="_IDGenText17" text="{UploadDocSrvTabData>ID}"/>
                        <Text id="_IDGenText13" text="{UploadDocSrvTabData>fileName}"/>
                        <HBox id="_IDGenHBox5" justifyContent="End" width="100%">
                          <Button id="_IDGenButton" icon="sap-icon://download" press="onDownloadTabAttachemnt" class="sapUiSmallMarginEnd">
                            <customData>
                              <core:CustomData key="fileName" value="{UploadDocSrvTabData>fileName}"/>
                              <core:CustomData key="ID" value="{UploadDocSrvTabData>ID}"/>
                            </customData>
                          </Button>
                          <Button id="_IDGenButton1" icon="sap-icon://delete" press="onDeleteTabAttchment" visible="{UploadDocSrvTabData>deleteTabVisible}">
                            <customData>
                              <core:CustomData key="fileName" value="{UploadDocSrvTabData>fileName}"/>
                              <core:CustomData key="ID" value="{UploadDocSrvTabData>ID}"/>
                            </customData>
                          </Button>
                        </HBox>
                      </cells>
                    </ColumnListItem>
                  </items>
                </Table>
              </VBox>
            </uxap:ObjectPageSubSection>
          </uxap:subSections>
        </uxap:ObjectPageSection>
      </uxap:sections>
    </uxap:ObjectPageLayout>
  </Page>
</mvc:View>

